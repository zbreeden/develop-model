{
  "output": "clinical_participants.csv",
  "rows": 1200,
  "seed": 420,
  "columns": {
    "participant_id": { "type": "uuid" },
    "first_name":     { "type": "first_name" },
    "last_name":      { "type": "last_name" },
    "email":          { "type": "email" },
    "city":           { "type": "city" },
    "state":          { "type": "state" },
    "zip":            { "type": "zip" },
    "dob":            { "type": "date", "start": "1955-01-01", "end": "2005-12-31" },
    "gender":         { "type": "choice", "values": ["Female","Male","Nonbinary","Prefer not to say"], "weights": [0.49,0.49,0.015,0.005] },
    "arm":            { "type": "choice", "values": ["Treatment","Control"], "weights": [0.5,0.5] },
    "enrolled_at":    { "type": "date", "start": "2023-01-01", "end": "2025-06-30" },
    "site_code":      { "type": "choice", "values": ["CIN-01","CIN-02","DAY-01","COL-01","LEX-01"], "weights": [0.35,0.25,0.15,0.15,0.10] },
    "is_withdrawn":   { "type": "bool", "p": 0.12 }
  }
}

